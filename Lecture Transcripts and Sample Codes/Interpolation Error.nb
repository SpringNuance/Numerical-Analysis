Cell[BoxData[{
 RowBox[{"max", "=", 
  RowBox[{"First", "@", 
   RowBox[{"Maximize", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"pr", ",", 
       RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", "1"}]}], "}"}], ",", 
     "x"}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"min", "=", 
  RowBox[{"First", "@", 
