Cell[BoxData[
 RowBox[{
  RowBox[{"pr", "=", 
   RowBox[{"Times", "@@", 
    RowBox[{"(", 
     RowBox[{"x", "-", "xp"}], ")"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8297943857130537`*^9, 3.829794398524962*^9}},
